    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numero1, numero2, numero3, maior" type="Integer" array="False" size=""/>
            <output expression="&quot;Determinar o maior dentre 3 n&#250;meros&quot;" newline="True"/>
            <output expression="&quot;Insira o primeiro n&#250;mero: &quot;" newline="True"/>
            <input variable="numero1"/>
