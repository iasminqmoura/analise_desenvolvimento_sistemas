                </then>
                <else>
                    <if expression="numero2 &gt;= numero1 &amp;&amp; numero2 &gt;= numero3">
                        <then>
                            <output expression="&quot;O maior n&#250;mero inserido &#233;: &quot;" newline="True"/>
